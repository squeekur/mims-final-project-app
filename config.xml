<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.berkeley.mimsFinalProjectAndroid" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EudaeSense</name>
    <description>
        MIMS Final Project
    </description>
    <author email="akester@berkeley.edu" href="http://mims.aprildawnkester.com">
        April Dawn Kester
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
     <preference name="SplashScreen" value="screen" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <!--added to fix stupid error -->
    <!-- Allow links to http:159.203.248.207-->
    <allow-navigation href="http://159.203.248.207:3000/*" />
    <allow-navigation href="https://159.203.248.207:4000/*" />
    <!-- Wildcards are allowed for the protocol, as a prefix to the host, or as a suffix to the path -->
    <allow-navigation href="*://*.http://159.203.248.207:3000/*" />
    <allow-navigation href="*://*.https://159.203.248.207:4000/*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <!--
            ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px
        -->
        <icon src="res/android/ldpi.png" density="ldpi" />
        <icon src="res/android/mdpi.png" density="mdpi" />
        <icon src="res/android/hdpi.png" density="hdpi" />
        <icon src="res/android/xhdpi.png" density="xhdpi" />
        <icon src="res/android/xxhdpi.png" density="xxhdpi" />
        <icon src="res/android/xxxhdpi.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
